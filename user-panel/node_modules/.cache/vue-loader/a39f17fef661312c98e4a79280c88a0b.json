w\">\n        <div class=\"container\">\n          <div class=\"col-md-6 ml-auto mr-auto\">\n            <div class=\"alert alert-danger\" dismissible v-if=\"!success\">Invalid Email or Password</div>\n            <div class=\"alert alert-warning\" dismissible v-if=\"!userVerified\">Please Verifiy your Email address to continue</div>\n              <Card class=\"login-card\">\n                <h3 class=\"text-center text-info\"><b>A People-Centric Marketplace</b></h3>\n                <form action=\"\" @submit=\"loginUser\">\n                <fg-input class=\"no-border input-lg\" addon-left-icon=\"now-ui-icons ui-1_email-85\" placeholder=\"Email\" v-model=\"userPayload.email\" type=\"email\"></fg-input>\n                <fg-input class=\"no-border input-lg\" addon-left-icon=\"now-ui-icons ui-1_lock-circle-open\" placeholder=\"Password\" v-model=\"userPayload.password\" type=\"password\"></fg-input>\n                <div class=\"card-footer text-center\">\n                <input type=\"submit\"\n                  @click=\"loginUser\"\n                  value=\"Login\"\n                  class=\"btn btn-info text-white btn-round btn-lg btn-block\"\n                  />\n                </div>\n                </form>\n              <div>\n                <h6>\n                  <router-link class=\"link footer-link pull-right text-success\" to=\"/register\">\n                    Need an Account? Register\n                  </router-link>\n                </h6>\n                <h6>\n                  <router-link class=\"link footer-link text-center text-success\" to=\"/forgot-password\">\n                    Forgot Password? Reset\n                  </router-link>\n                </h6>\n              </div>\n              </Card>\n          </div>\n        </div>\n      </div>\n    </div>\n    \n  </div>\n</template>\n<script>\nimport {mapGetters, mapActions} from 'vuex';\nimport { Card, Button, FormGroupInput } from '@/components';\nimport MainFooter from '@/layout/MainFooter';\nexport default {\n  name: 'login-page',\n  bodyClass: 'login-page',\n  components: {\n    Card,\n    MainFooter,\n    [Button.name]: Button,\n    [FormGroupInput.name]: FormGroupInput\n  },\n  computed: { ...mapGetters(['loggedUser', 'verified', 'error'])},\n  data() {\n    return {\n      success: true,\n      userVerified: true,\n      userPayload: {\n        email: 'arslanahmed983@gmail.com',\n        password: '123456',\n      },\n      \n    }\n  },\n  \n  methods: {\n    ...mapActions(['authenticateLogin']),\n\n    loginUser() {\n      this.authenticateLogin(this.userPayload);\n    }\n  },\n  watch: {\n    error (val) {\n      if(val) {\n        console.log(val)\n        this.success = false\n      }\n    },\n    loggedUser (val) {\n        //&& val.uid.length > 0\n        console.log(\"auth state watched\")\n\n            if (val) {\n                setTimeout(() => {\n                  console.log('VAL',val)\n                  this.$router.push('/events')\n                }, 500)\n            }\n            else{\n              this.success = false\n              console.log('Something wrong')\n            }\n        },\n        verified (val) {\n        //&& val.uid.length > 0\n            if (val) {\n                setTimeout(() => {\n                  if(val == 'no'){\n                    this.userVerified = false\n                  }\n                }, 500)\n            }\n            else{\n              this.success = false\n              console.log('Something wrong')\n                // this.$router.push({path:'/'})\n            }\n        },\n  },\n};\n</script>\n<style scoped>\n  #button {\n    margin-top: -85px;\n  }\n  .inputField {\n      margin-top: -35px;\n  }\n  .login-card {\n    margin-top:100px;\n  }\n</style>\n"]}]}